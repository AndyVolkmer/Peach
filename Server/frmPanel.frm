Private Sub ListView1_ItemCheck(ByVal Item As MSComctlLib.ListItem)
If Item.Checked Then
    With ListView1.ListItems
        For i = 1 To .Count
            .Item(i).Checked = False
        Next i
    End With
    ListView1.SelectedItem = Item
    Item.Checked = True
Else
    Item.Checked = False
End If
End Sub
